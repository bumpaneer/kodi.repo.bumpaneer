<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="kodi.repo.bumpaneer"
    name="Bumpaneer's Repository"
    version="1.0.2"
    provider-name="Bumpaneer">
    <requires>
      <import addon="xbmc.addon" version="12.0.0"/>
    </requires>
    <extension point="xbmc.addon.repository"
      name="Bumpaneer's Add-on Repository">
      <dir minversion="14.0.0">
        <info compressed="true">http://github.com/bumpaneer/kodi.repo.bumpaneer/raw/master/helix/addons.xml</info>
        <checksum>http://github.com/bumpaneer/kodi.repo.bumpaneer/raw/master/helix/addons.xml.md5</checksum>
        <datadir zip="true">https://github.com/bumpaneer/kodi.repo.bumpaneer/raw/master/helix</datadir>
        <hashes>true</hashes>
      </dir>
      <dir minversion="15.0.0">
        <info compressed="true">http://github.com/bumpaneer/kodi.repo.bumpaneer/raw/master/isengard/addons.xml</info>
        <checksum>http://github.com/bumpaneer/kodi.repo.bumpaneer/raw/master/isengard/addons.xml.md5</checksum>
        <datadir zip="true">https://github.com/bumpaneer/kodi.repo.bumpaneer/raw/master/isengard</datadir>
        <hashes>true</hashes>
      </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary lang="en">Bumpaneers's Add-ons for XBMC</summary>
      <description lang="en">Download and install Add-ons for XBMC.</description>
      <disclaimer lang="en">-</disclaimer>
      <platform>all</platform>
    </extension>
</addon>


<addon
	id="skin.unity"
	version="0.4.2"
	name="Unity"
	provider-name="Eddage, Bumpaneer">
	<requires>
		<import addon="xbmc.gui" version="5.9.0"/>
		<import addon="script.skinshortcuts" version="0.6.5"/>
	</requires>
	<extension
		point="xbmc.gui.skin"
		debugging="false"
		effectslowdown="1.0">
		<res width="1920" height="1080" aspect="16:9" default="true" folder="1080i" />
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">A new skin for Kodi 13 Gotham by Eddage</summary>
		<summary lang="en_AU">A new skin for Kodi 13 Gotham by Eddage</summary>
		<summary lang="en_NZ">A new skin for Kodi 13 Gotham by Eddage</summary>
		<summary lang="en_US">A new skin for Kodi 13 Gotham by Eddage</summary>
		<description lang="en">Unity is an adaptation of Confluence based on a material design.</description>
		<description lang="en_AU">Unity is an adaptation of Confluence based on a material design.</description>
		<description lang="en_NZ">Unity is an adaptation of Confluence based on a material design.</description>
		<description lang="en_US">Unity is an adaptation of Confluence based on a material design.</description>
		<disclaimer lang="en">More info on the skin can be found at the Kodi Forums.</disclaimer>
		<disclaimer lang="en_AU">More info on the skin can be found at the Kodi Forums.</disclaimer>
		<disclaimer lang="en_NZ">More info on the skin can be found at the Kodi Forums.</disclaimer>
		<disclaimer lang="en_US">More info on the skin can be found at the Kodi Forums.</disclaimer>
		<license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
		<forum>http://forum.kodi.tv/forumdisplay.php?fid=244</forum>
		<source>https://github.com/bumpaneer/skin.unity/tree/Isengard</source>
		<platform>all</platform>
	</extension>
</addon>
</addons>
